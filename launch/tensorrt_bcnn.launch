<launch>
  <arg name="input_topic" default="/image_raw"/>
  <node pkg="tensorrt_bcnn" type="tensorrt_bcnn"
    name="$(anon tensorrt_bcnn)" output="screen" >
    <rosparam>
      points_src: points_raw
      trained_model: bcnn_all_0122.engine
      score_threshold: 0.8
    </rosparam>
  </node>
</launch>
